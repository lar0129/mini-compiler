int main(){
    int a=0;
    int count=0;
    while(a<=0){
        a=a-1;
        count=count+1;
        if(a<-20){
            a=3;
            break;
        }
        else{
            while(1){
                a=a+1;
                count=count+1;
                if(a>0){
                    a=0;
                    break;
                }
            }
            a=0;
            break;
        }
    }
    return count;
}
